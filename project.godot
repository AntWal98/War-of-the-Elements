; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="War of the Elements"
run/main_scene="res://Scenes/Interface/main_menu.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

Global="*res://Scripts/Globals/Global.gd"

[display]

window/size/viewport_width=640
window/size/viewport_height=480
window/size/window_width_override=1280
window/size/window_height_override=720
window/stretch/mode="canvas_items"

[file_customization]

folder_colors={
"res://Assets/": "pink",
"res://Scenes/": "green",
"res://Scripts/": "yellow"
}

[gui]

theme/custom="res://Assets/themes/MinimalUI.tres"
theme/custom_font="res://Assets/Fonts/OpenSans-Regular.ttf"

[rendering]

textures/canvas_textures/default_texture_filter=0
renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
environment/defaults/default_clear_color=Color(0.144063, 0.144063, 0.144062, 1)
